<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <h5 class="card-header text-center" style=background-color:lightblue>Questions About CSS</h5>
                <div class="card-body">
                    <h5 class="card-title">
                        <form class="create-form">
                            <div class="form-group">
                                <label for="post">Question:</label>
                                <input type="text" id="quest" name="quest">
                            </div>
                            <button type="submit">Add Post</button>
                        </form>
                    </h5>
                </div>
            </div>
            <div class="container jsPosts">
                {{#each posts}}
                <div class="card">
                    <div class="card-header">
                        <div class="card-body">
                            <p>{{this.question}}</p>

                            <p>
                                <a class="btn btn-primary" data-toggle="collapse" href="#multiCollapseExample{{id}}"
                                    role="button" aria-expanded="false"
                                    aria-controls="multiCollapseExample{{id}}">Responses</a>
                            </p>
                            <div class="row">
                                <div class="col">
                                    <div class="collapse multi-collapse" id='multiCollapseExample{{id}}'>
                                        <div class="card card-body">
                                            <form class="create-formA" data-id="{{id}}">

                                                <div class="form-group">
                                                    <label for="post">New Reply:</label>
                                                    <input type="text" id="answer" name="answer">
                                                </div>
                                                <button type="submit">Add Reply</button>
                                            </form>
                                            <br>
                                            <h4 style="text-align: center;"> Answers: </h4>
                                            {{#each Responses}}
                                            <div class="card card-body">
                                                <p>{{this.answer}}</p>
                                            </div>
                                            {{/each}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>
